{
  "entrypoint": "vapeswag/index.html",
  "shell": "vapeswag/src/shop-app.html",
  "fragments": [
    "vapeswag/src/shop-list.html",
    "vapeswag/src/shop-detail.html",
    "vapeswag/src/shop-cart.html",
    "vapeswag/src/shop-checkout.html",
    "vapeswag/src/lazy-resources.html"
  ],
  "sources": [
    "vapeswag/src/**/*",
    "vapeswag/data/**/*",
    "vapeswag/images/**/*",
    "vapeswag/bower.json"
  ],
  "extraDependencies": [
    "vapeswag/app.yaml",
    "vapeswag/manifest.json",
    "vapeswag/bower_components/webcomponentsjs/*"
  ],
  "lint": {
    "rules": ["polymer-2-hybrid"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
